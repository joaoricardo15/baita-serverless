{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "User",
  "description": "Schema for a user object",
  "type": "object",
  "properties": {
    "userId": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "picture": {
      "type": "string"
    }
  },
  "required": ["userId", "name", "email", "picture"]
}
